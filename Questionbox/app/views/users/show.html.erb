
<h1><%= @user.username %>'s Questions</h1>
<table class = "table">
  <thead>
    <tr>
      <th>Username</th>
      <th>Question</th>
    </tr>
  </thead>
  <tbody>
  <% @user.questions.each do |question| %>
    <tr>
      <td><%= @user.username %></td>
      <td><%= question.body %></td>
    </tr>
  <% end %>
  </tbody>
</table>


<%= link_to 'Back', root_path %>